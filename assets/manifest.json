{
	"manifest_version": 2,

	"name": "__MSG_appName__",
	"description": "__MSG_appDesc__",
	"version": "0.1.4",

	"default_locale": "en",
	"icons": {
		"16": "discord16.png",
		"48": "discord48.png",
		"128": "discord128.png"
	},
	"options_page": "options.html",
	"background": {
		"scripts": ["background.bundle.js"]
	},
	"permissions": [
		"notifications",
		"storage",
		"tabs",
		"*://discord.com/*"
	]
}
